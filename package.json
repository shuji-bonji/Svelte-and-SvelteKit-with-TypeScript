{
  "name": "svelte-and-sveltekit-with-typescript",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "NODE_ENV=production vite build --logLevel=error && npx pagefind --site dist && node scripts/postbuild.js",
    "preview": "vite preview",
    "postbuild": "node scripts/postbuild.js && node scripts/generate-sitemap.js",
    "security-check": "node scripts/check-security.js",
    "update-safe": "npm update && npm audit fix && npm run build",
    "check-outdated": "npm outdated"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
    "@sveltepress/theme-default": "^7.0.2",
    "@sveltepress/vite": "^1.3.1",
    "@types/node": "^24.10.1",
    "pagefind": "^1.3.0",
    "svelte": "^5.0.0",
    "svelte-sitemap": "^2.7.1",
    "typescript": "^5.3.3",
    "vite": "^7.2.7"
  },
  "dependencies": {
    "mermaid": "^11.9.0"
  }
}
